{
  "name": "@marufzak/react",
  "version": "1.0.0",
  "description": "Zero-dependency, lightweight and minimalistic implementation of the React library",
  "keywords": [
    "react",
    "ui",
    "library",
    "no-dependencies",
    "lightweight"
  ],
  "main": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "scripts": {
    "watch": "NODE_ENV=development rollup -c -w",
    "build": "NODE_ENV=production rollup -c"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.9",
    "@types/prop-types": "^15.7.12",
    "csstype": "^3.1.3",
    "prettier": "^3.3.2",
    "prop-types": "^15.8.1",
    "rollup": "^4.18.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "tslib": "^2.6.3"
  },
  "author": {
    "email": "marufzak04@gmail.com",
    "name": "Ma'ruf Zakirov",
    "url": "https://github.com/MarufZak"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./types": {
      "default": "./dist/types/index.d.ts",
      "types": "./dist/types/index.d.ts"
    },
    "./dom": {
      "default": "./dist/dom/index.js",
      "types": "./dist/dom/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types/index.d.ts"
      ],
      "dom": [
        "./dist/dom/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ]
}
