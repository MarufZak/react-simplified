{
  "name": "@marufzak/react",
  "version": "0.8.4",
  "description": "Zero-dependency, lightweight and minimalistic implementation of the React library",
  "keywords": [
    "react",
    "ui",
    "library",
    "no-dependencies",
    "lightweight"
  ],
  "main": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "scripts": {
    "watch": "NODE_ENV=development rollup -c -w",
    "build": "NODE_ENV=production rollup -c"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.10",
    "@types/react": "^18.3.1",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "rollup-plugin-delete": "^2.0.0",
    "tslib": "^2.6.2"
  },
  "author": {
    "name": "marufzak"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./types": {
      "default": "./dist/types/index.d.ts",
      "types": "./dist/types/index.d.ts"
    },
    "./dom": {
      "default": "./dist/dom/index.js",
      "types": "./dist/dom/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types/index.d.ts"
      ],
      "dom": [
        "./dist/dom/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@rollup/plugin-replace": "^5.0.7"
  }
}
