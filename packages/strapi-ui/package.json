{
  "name": "@marufzak/strapi-ui",
  "version": "1.0.0",
  "description": "Headless UI components built with @marufzak/react and Tailwind CSS packages",
  "main": "./dist/components/index.js",
  "types": "./dist/components/index.d.ts",
  "scripts": {
    "watch": "NODE_ENV=development rollup -c -w",
    "build": "NODE_ENV=production rollup -c"
  },
  "author": {
    "email": "marufzak04@gmail.com",
    "name": "Ma'ruf Zakirov",
    "url": "https://github.com/MarufZak"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "glob": "^11.0.0",
    "postcss": "^8.4.45",
    "rollup": "^4.21.2",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.12",
    "tslib": "^2.7.0"
  },
  "peerDependencies": {
    "@marufzak/react": "1.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./icons": {
      "default": "./dist/icons/index.js",
      "types": "./dist/icons/index.d.ts"
    },
    "./icons/logos": {
      "default": "./dist/icons/logos/index.js",
      "types": "./dist/icons/index.d.ts"
    },
    "./utils": {
      "default": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/components/index.d.ts"
      ],
      "icons": [
        "./dist/icons/index.d.ts"
      ],
      "icons/logos": [
        "./dist/icons/logos/index.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  }
}
